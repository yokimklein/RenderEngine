  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='DX12_Release|x64'">
    <LocalDebuggerWorkingDirectory>$(OutputPath)</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
  </PropertyGroup>
